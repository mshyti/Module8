import unittest
import assign_average as test

class MyTestCase(unittest.TestCase):
    def test_Aa(self):
        self.assertEqual("one", test.switch_average('A'))
        self.assertEqual("one", test.switch_average('a'))


if __name__ == '__main__':
    unittest.main()

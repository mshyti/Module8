import unittest
import assign_average as test

class MyTestCase(unittest.TestCase):
    def test_Bb(self):
        self.assertEqual("two", test.switch_average('B'))
        self.assertEqual("two", test.switch_average('b'))

if __name__ == '__main__':
    unittest.main()

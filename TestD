import unittest
import assign_average as test

class MyTestCase(unittest.TestCase):
    def test_Dd(self):
        self.assertEqual("four", test.switch_average('D'))
        self.assertEqual("four", test.switch_average('d'))

if __name__ == '__main__':
    unittest.main()

def switch_average(key):
    switcher = {
        'A': "one",
        'a': "one",
        'B': "two",
        'b': "two",
        'C': "three",
        'c': "three",
        'D': "four",
        'd': "four",
        'E': "five",
        'e': "five"
    }
    return switcher.get(key, "wrong key")
